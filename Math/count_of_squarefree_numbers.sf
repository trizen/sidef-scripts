# Sub-linear formula for counting the number of squarefree numbers <= n.

# See also:
#   https://oeis.org/A013928

func squarefree_count(n) {
    gather {
        n.isqrt.each_squarefree {|k|
            take(moebius(k) * idiv(n, k*k))
        }
    }.sum
}

for k in (1..9) {
    say "S(10^#{k}) = #{squarefree_count(10**k)}"
}

__END__
